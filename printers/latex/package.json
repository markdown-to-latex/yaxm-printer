{
    "name": "@md-to-latex/yaxm-printer-latex",
    "version": "1.0.0-beta.7",
    "description": "Latex Printer for Yet Another eXtended Markdown",
    "main": "dist/index.js",
    "scripts": {
        "prebuild": "npm run convert-schema",
        "build": "tsc --build --pretty",
        "watch": "tsc --build --watch --pretty",
        "test": "jest test",
        "convert-schema": "node ./utils/convert-schema.js && prettier --write src/config/types.ts"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/markdown-to-latex/yaxm-printers"
    },
    "keywords": [
        "markdown",
        "latex",
        "converter",
        "transpiler",
        "report"
    ],
    "files": [
        "dist/**",
        "*.schema.json"
    ],
    "author": "Anatolii Titov <a@toliak.ru>",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^20.11.30",
        "jest": "^29.7.0",
        "json-schema-to-typescript": "^13.1.2",
        "prettier": "^3.2.5",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.3",
        "@md-to-latex/converter": "^2.0.0-beta.14",
        "utils__": "file:../../utils"
    },
    "dependencies": {
        "js-yaml": "^4.1.0",
        "json5": "^2.2.3",
        "jsonschema": "^1.4.1"
    }
}
